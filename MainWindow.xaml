        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SmartRockets"
        mc:Ignorable="d"
        Title="Smart Rockets" Height="450" Width="450">
    <Grid>
        
        <Button x:Name="startBtn" Content="Start" HorizontalAlignment="Left" Margin="137,5,0,0" VerticalAlignment="Top" Width="50" Click="StartBtn_Click"/>
        <Button x:Name="stopBtn" Content="Stop" HorizontalAlignment="Left" Margin="250,5,0,0" VerticalAlignment="Top" Width="50" Click="StopBtn_Click"/>
        <Canvas x:Name="canvas" HorizontalAlignment="Left" Height="397" VerticalAlignment="Top" Width="443" Margin="0,23,0,0"/>
            
        
    </Grid>
</Window>
